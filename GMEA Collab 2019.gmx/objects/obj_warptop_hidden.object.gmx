            <string>///Warp from the top but only if the arrow is showing

//If visible
if (visible) {
    
    player = collision_rectangle(x+16,y-2,x+16,y+2,obj_playerparent,0,0);
    if (player)
    &amp;&amp; (keyboard_check(global.downkey)) {
    
        //Play 'Warp' sound
        audio_play_sound(snd_warp, 0, false);
        
        //Create pipe player
        with (instance_create(x+16,y-16,obj_player_warp)) {
            
            //Set direction
            direction = 270;
            
            //Facing direction
            image_xscale = other.player.xscale;
            
            //Flashing
            isflashing = other.player.isflashing;
        }
        
        //Destroy player
        with (player) instance_destroy();
    }
}

//Update anim
anim += 0.15;

//Make visible if the "!" event is active
if (obj_levelcontrol.alarm[11] != -1)
    visible = 1;
else
    visible = 0;
</string>
