        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw the HUD

//Disable the fog
if (instance_exists(obj_lightningsheet)) then d3d_set_fog(0, c_black, 0, 64);

//Reserve Box / Health Meter
if (global.healthmode == 0) {

    //Reserve item
    if (global.reserve &gt; cs_small) {
    
        draw_sprite_ext(constant_get_sprite(global.reserve), 0, view_xview[0]+view_wview[0]/2, view_yview[0]+13, 1, 1, 0, c_white, 1);
    }    
    
    //Box
    if (global.level != -1)
        draw_sprite_ext(spr_hud_reserve, 0, view_xview[0]+view_wview[0]/2, view_yview[0]+6, 1, 1, 0, c_white, 1);
    else
        draw_sprite_ext(spr_hud_reserve, 1, view_xview[0]+view_wview[0]/2, view_yview[0]+6, 1, 1, 0, c_white, 1);
}

//Star coins
if (global.level != -1) {

    for (var i = 0; i &lt; 3; i++) {
    
        //Draw the coin if it has been collected
        if (ds_map_find_value(global.sc[i],global.level) &gt; 0)
            draw_sprite_ext(spr_hud_starcoin_get, -1, view_xview[0]+112+(16*i), view_yview[0]+32, 1, 1, 0, c_white, 1);
    }
}

//Set font
draw_set_font(global.numbers_w);

//Coins
draw_text(view_xview[0]+133, view_yview[0]+15, string_format(global.coins, 4, 0));

//Lives
draw_text(view_xview[0]+241, view_yview[0]+20, string_format(lives, 2, 0));

//P-Meter
if (instance_exists(obj_playerparent)) {

    draw_sprite_part_ext(spr_hud_pmeter, flash, 0, 0, 46/112*obj_playerparent.pmeter, 4, view_xview[0]+210, view_yview[0]+40, 1, 1, c_white, 1);
}

//Time
if (global.time &gt; 0) {

    //Draw the bomb
    draw_sprite_ext(spr_hud_time, 0, view_xview[0], view_yview[0]+203, 1, 1, 0, c_white, 1);
    
    //Draw the fuse
    draw_sprite_part_ext(spr_hud_time_fuse, 0, 0, 0, 364/global.timemax*global.time, 5, view_xview[0]+14, view_yview[0]+208, 1, 1, c_white, 1);
    draw_sprite_ext(spr_hud_time_fusefire, anim, view_xview[0]+14+(364/global.timemax*global.time), view_yview[0]+208, 1, 1, 0, c_white, 1);
    
    //Draw time if there's 100 seconds left
    if (global.time &lt; 101) {
    
        //Set the font
        draw_set_font(global.numbers_g);
    
        //Draw the text
        draw_text_colour(view_xview[0]+16, view_yview[0]+195, global.time, c_red, c_red, c_red, c_red, 1);
    }
}
    
//Enable the fog
if (instance_exists(obj_lightningsheet)) then d3d_set_fog(1, c_black, 0, 64);
</string>
