            <kind>1</kind>
            <string>///Horizontal Block Platform

//Create slow spark effects
var a = 0;
repeat (8) {

    with (instance_create(x, y, obj_smoke)) {
        
        //Motion
        motion_set(a,0.5);
    }
    a += 45;
}

//Facing direction
ready = 1;
